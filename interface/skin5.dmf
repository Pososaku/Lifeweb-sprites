		statusbar = false
		is-maximized = true
		icon = 'logo.png'
		macro = "macro"
		menu = "menu"
	elem "mainvsplit"
		type = CHILD
		pos = 0,0
		size = 640x440
		anchor1 = 0,0
		anchor2 = 100,100
		background-color = #100c0c
		saved-params = "splitter"
		right = "rpane"
		is-vert = true
		splitter = 75

